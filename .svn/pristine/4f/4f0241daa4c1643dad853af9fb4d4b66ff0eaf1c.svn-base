@CHARSET "UTF-8";
*{margin:0px;padding:0px;}
html{
	height: 100%;
}
body{font-family: "Hiragino Sans GB","Microsoft Yahei",arial,\5b8b\4f53,"Helvetica Neue",Helvetica,STHeiTi,sans-serif;background-color:#fff;}
li{list-style:none;}
a{text-decoration:none !important;color:#ededed;}
a:hover{color:orange;}
p{margin:0px;}
iframe{border:none;}
header{
	height: 80px;
    background: #364150;
    color: #fff;
    width: 100%;
    position: fixed;
    z-index: 10;
    box-shadow: 0px 10px 10px #cdc8ea;
    top: 0px;
    
}
.content{
	max-width:1000px;
	margin:0 auto;
	height: 100%;
}
header .logo{
	font-size: 35px;
    font-weight: bold;
    font-family: 楷体;
    float: left;
    margin-top: 20px;
    margin-left: 10px;
    color: #f9f91d;
    text-shadow: 1px 1px 1px #666;
}
header nav{
	width:75%;
	display: inline-block;
}
header nav .first{
	overflow:hidden;
	padding-left: 10px;
}
header nav .first{
	overflow: hidden;
    margin-left: 50px;
    padding-top: 10px;
}
header nav .first li{
	display: block;
    position: relative;
    float: left;
    font-size: 25px;
    line-height: 62px;
    margin-left: 3px;
    text-align: center;
    width: 100px;
    cursor: pointer;
    margin-top: 10px;
}
header nav .first li:hover{
	color:orange;
	border-bottom:5px dashed orange;
	background:rgba(214, 209, 209, 0.45);
}
.active{
	color:orange !important;
	box-shadow: 0px 0px 2px 2px #fff;
}
.active a{
	color:orange !important;
}

header nav .second{
	
	
	display: none;
	overflow:hidden;
}
header nav .second{
	
	z-index: 99999;
	display: none;
}
header nav .second li{
    font-size: 17px;
    width: 90px;
    cursor: pointer;
    padding:2px;
    border-bottom: 1px solid #fff;
    float: none;
    margin-top: 3px;
    line-height:30px;
    background: #00000052;
}
header nav .second li:hover{
	
    font-size: 17px;

    cursor: pointer;
    padding: 2px;
    border-bottom: 1px solid #fff;
    float: none;
    box-shadow: 0px 0px 5px 2px #242548;
}
header nav .second li a{
	color:orange;
	font-size: 16px;
}
header nav .second li a:hover{
	font-size: 17px;
	color: #f4cedf;
    text-shadow: 0px 0px 3px #090909;
}
header .userInfo{
	position:fixed;
    top:22px;
    right:10px;
    cursor: pointer;
}
header .userInfo li{
	color: #eee;
    font-size: 17px;
    text-align: center;
    padding:2px;
}
header .userInfo a{
	color: #fff;
	margin: 0px 10px;
	text-align: center;
}
header .userInfo #login{
	margin-right:30px;
}
header .userInfo #userInfo{
	margin-right:20px;
}
header .userInfo a:hover{
	box-shaow:0px 0px 5px 1px #000;
}
header .userInfo a:hover{
	color: orange;
	text-decoration:underline;
}
.main{
	margin-top:100px;
	margin-bottom:20px;
	position:relative;
}
.index .mainContent .productList{
	    background: #fff;
    	width: 100%;
    	color:#000;
}
.index .productList .header{
	padding-top:4px;
	border-bottom:1px solid #ddd;
}
.index .productList .header p{
	font-size: 22px;
    color: #bf2121;
    margin:0px;
    
}
.productList{
	
	padding-top:10px;
	padding-bottom:10px;
}
.productList .list ul{overflow:hidden;padding:5px;}
.productList .list img{
	 width: 210px;
   	 height: 210px;
   	 margin-left:10px;
	transition:all 0.5s linear;
	-moz-transition:all 0.5s linear;
	-webkit-trasform:all 0.5 linear;
}
.productList .list ul li:hover img{
	transform:rotateY(180);
	-moz-transform:rotateY(180deg);
	-webkit-transform:rotateY(180deg);
}
 .productList .list ul li{list-style:none;float:left;width:238px;padding:5px;margin-top:5px;position:relative;}
.productList .list li:hover{box-shadow:0px 0px 3px 5px #ddd;}
.productList .list .s_price{font-size:20px;color:#c00;}
.productList .list  .s_title{font-size:14px;line-height:30px;}
.productList .list  .s_btn{width:228px;height:36px;display:block;background:#c00;border-radius:3px !important;text-align:center;line-height:36px;color:#fff;font-size:14px;text-decoration:none;margin-left: 4px;}
.productList .list  .s_btn:hover{background:#900;}
.bgfooter{
    background: #364150;
    width:100%;
    color:#888;
	font-size:14px;
	line-height:40px;
	padding-top:10px;
}
.bgfooter ul{
	max-width:1000px;
	margin:0 auto;
}
.bgfooter a{
	text-decoration:none !important;
	color:#888;
	font-size:14px;
}
.bgfooter p{
	margin:0 auto;
	width:600px;
	
}
.product{
	color:#999;
}
.product img{
	width:135px !important;
	height:135px !important;
	border-radius:50% !important;
	transition:all 0.5s linear;
	-moz-transition:all 0.5s linear;
	-webkit-trasform:all 0.5 linear;
}
.product li{
	text-align:center;
	
	overflow:hidden;
}
.product li:hover img{
	transform:rotateY(180);
	-moz-transform:rotateY(180deg);
	-webkit-transform:rotateY(180deg);
	
}
.product .t_name{
	color:#666;
	font-size:20px;
}
.product .t_name a{
	color:orange;
	font-size:20px;
}
.product .t_title{
	border-bottom:1px solid #999;
	padding-bottom:10px;
	margin-bottom:10px;
}
.loginBg{
	background-color:#069194;
	width:100%;
	max-width:100%;
	margin:0px;
	margin-top:80px;
	position:relative;
}
.loginBgCenter{
	background:url("img/bg.png");
	width:1000px;
	height:477px;
	margin:0 auto;
}
.loginBox{
	position:absolute;
	right:24px;
	top:45px;
	background:rgba(255,255,255,0.40);
	width:350px;
	height:365px;
	box-shadow: 0px 0px 12px 1px #fff;
}