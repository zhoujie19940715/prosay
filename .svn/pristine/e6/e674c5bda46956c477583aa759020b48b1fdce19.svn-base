*{margin:0;padding:0}
	.banner{
		width:590px;
		height:340px;
		position:relative;
		overflow:hidden;
		margin:0 auto;
	}
	.banner .pannel{
		width:100%;
		height:100%;
		overflow:hidden;
	}
	.banner .pannel .scroll{
		width:10000px;
		height:340px;
		position:absolute;
		left:0px;

	}
	.banner .pannel .scroll a{
		float:left;
	}
	.banner .btns{
		width:100%;
		text-align:center;
		position:absolute;
		bottom:10px;
	}
	.banner .btns span{
		display:inline-block;
		width:12px;
		height:12px;
		cursor:pointer;
		background:url("images/but1.png");
		margin-left:5px;
	}
	.banner .btns span:hover{
		width:16px;
		height:16px;
		background:url("images/but2.png");
	}
	.banner .btns .active{
		width:16px;
		height:16px;
		background:url("images/but2.png");
	}
	.banner .next,.banner .prev{
		display:block;
		width:70px;
		height:70px;
		position:absolute;
		top:135px;
	}
	.banner .prev{
		left:0px;
		background:url("images/aprev.png");
	}
	.banner .next{
		background:url("images/anext.png");
		right:0px;
	}
	.banner .prev:hover{
		background:url("images/prev.png");
	}
	.banner .next:hover{
		background:url("images/next.png");
		
	}